{
    "version": 2,
    "builds": [
      {
        "src": "src/app.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["src/**/*.ts"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ],
    "engines": {
      "node": "20.6.1"
    }
  }
  